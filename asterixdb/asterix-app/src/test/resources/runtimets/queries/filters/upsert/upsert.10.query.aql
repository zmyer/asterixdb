/*
 * Select the updated record through the btree access
 */
use dataverse test;

for $m in dataset('Tweet')
where $m.'text'= "Just posted a photo @ Campus Martius Park https://t.co/5Ax4E2CdWZ"
return $m.geo_tag.stateID